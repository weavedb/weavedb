{
  "name": "wdb-core",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build:cjs": "babel src --out-dir dist/cjs --config-file ./.babelrc-cjs",
    "build": "rm -rf dist && npm run build:cjs && cp src -rf dist/esm && node make.js && cp .npmignore dist/",
    "bundle": "esbuild src/db.js --bundle --format=esm --outfile=wdb.min.js --minify && brotli-cli compress wdb.min.js",
    "deploy": "node script/publish.js"
  },
  "exports": {
    ".": {
      "require": "./cjs/index.js",
      "import": "./esm/index.js"
    },
    "./utils": {
      "require": "./cjs/utils.js",
      "import": "./esm/utils.js"
    },
    "./planner": {
      "require": "./cjs/planner.js",
      "import": "./esm/planner.js"
    },
    "./indexer": {
      "require": "./cjs/indexer.js",
      "import": "./esm/indexer.js"
    },
    "./fpjson": {
      "require": "./cjs/fpjson.js",
      "import": "./esm/fpjson.js"
    },
    "./devs": {
      "require": "./cjs/devs.js",
      "import": "./esm/devs.js"
    },
    "./preset": {
      "require": "./cjs/preset.js",
      "import": "./esm/preset.js"
    }
  },
  "devDependencies": {
    "@ar.io/sdk": "^3.21.0",
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-modules-commonjs": "^7.24.8",
    "@babel/preset-env": "^7.25.3",
    "@dha-team/arbundles": "^1.0.4",
    "esbuild": "^0.25.10",
    "yargs": "^18.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@lancedb/lancedb": "^0.21.2",
    "arjson": "^0.0.13",
    "brotli": "^1.3.3",
    "brotli-cli": "^2.1.1",
    "fast-sha256": "^1.3.0",
    "fpjson-lang": "^0.1.5",
    "hbsig": "^0.2.8",
    "http-message-signatures": "^1.0.4",
    "jsonschema": "^1.5.0",
    "monade": "^0.0.6",
    "node-sql-parser": "^5.3.11",
    "ramda": "^0.31.3",
    "zkjson": "^0.8.4"
  }
}
